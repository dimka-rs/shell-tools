alias  auu="sudo aptitude update ; sudo aptitude dist-upgrade ; sudo aptitude autoclean ; aptitude search '~N' ; sudo aptitude forget-new; sudo apt autoremove"
alias cd..='cd ..'
alias  cdo="cd $OLDPWD"
alias  cdh='cd ~/'
alias   la='ls -A'
alias    l='ls -CF'
alias   ll='ls -alF'
alias   pa='ps -Af'
alias   pg='ps auxw|grep'
